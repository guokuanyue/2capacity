.day-titlebg {
  background-color: #2b74cd;
}
.month-titlebg {
  background-color: #2ba9cd;
}
.year-titlebg {
  background-color: #592dbb;
}
.now-titlebg {
  background-color: #6b1eac;
}
.report {
  width: 85%;
  margin: 0 auto;
  min-width: 1050px;
}
.report .report-title {
  height: 40px;
  line-height: 40px;
  width: 97.5%;
  padding-left: 2.5%;
  color: white;
  margin-bottom: 20px;
  background-color: #303740;
}
.report .report-wrap {
  width: 100%;
  margin-bottom: 20px;
  display: flex;
}
.report .report-wrap .report-con-box {
  width: 24%;
  min-width: 300px;
  margin-right: 1%;
}
.report .report-wrap .report-con-box .report-con-title {
  width: 100%;
  height: 60px;
  line-height: 60px;
  text-align: center;
  color: #fff;
}
.report .report-wrap .report-con-box .repot-con-con {
  width: 100%;
  padding-bottom: 30px;
  background-color: #25292e;
  padding-top: 9.7%;
}
.report .report-wrap .report-con-box .repot-con-con .select-factory {
  width: 85.55%;
  height: 40px;
  border-radius: 5px;
  border: 1px solid #535960;
  background: #25292e;
  color: #adb6c2;
  margin-left: 7%;
  margin-bottom: 12%;
}
.report .report-wrap .report-con-box .repot-con-con .report-dateRange .el-date-editor--daterange.el-input__inner {
  width: 85.55%;
  margin-left: 7%;
  background: #25292e;
  border: 1px solid #535960;
  padding-right: 0px;
  margin-bottom: 12%;
}
.report .report-wrap .report-con-box .repot-con-con .report-con-btn .report-btn {
  border: 1px solid #535960;
  background: #25292e;
  color: #adb6c2;
  margin-left: 7%;
  padding: 2.5% 4%;
  border-radius: 5px;
}
.report .report-wrap .report-con-box .repot-con-con .report-con-btn .report-btn:hover {
  background-color: #2A84F6;
  color: #fff;
  border: 1px solid #2A84F6;
}
.report .report-wrap .report-con-box .repot-con-con .report-date {
  margin-bottom: 12%;
  margin-left: 7%;
}
.report .report-wrap .report-con-box .repot-con-con .repot-con-first-title {
  margin-top: -7%;
  padding: 0 0 2% 7%;
  color: #adb6c2;
}
.report .report-wrap .report-con-box .repot-con-con .report-el-cascader {
  padding: 0 0 2% 7%;
  color: #adb6c2;
}
.report .report-wrap .report-con-box .repot-con-con .report-con-btn2 {
  margin-top: 10%;
}
